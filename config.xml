<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application popup="true" title="Campost map" logo="/img/logo/campost.jpg" help="mviewer_help.html" measuretools="true" exportpng="true" style="css/themes/campost.css" togglealllayersfromtheme="true"/>    
    <mapoptions maxzoom="19" projection="EPSG:3857" center="1160896.92170965, 610464.3263995602" zoom="15" projextent="312168.8235166598, 5434978.459189172, 899205.2007468134, 5732777.121388218" />    
  
    <baselayers style="default"><!-- style="default"||gallery" -->
        <baselayer  type="OSM" id="darkmatter" label="Dark Matter" title="CartoDb" thumbgallery="img/basemap/darkmatter.png" 
            url="http://{a-c}.basemaps.cartocdn.com/dark_all/{z}/{x}/{y}.png" maxzoom="20" 
            visible="true" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
        <baselayer  type="OSM" id="pitney-bowes-streets" label="Pitney Bows Streets" title="CartoDb" thumbgallery="img/basemap/pitney-bowes-streets.png" 
            url="http://{a-c}.basemaps.cartocdn.com/pitney-bowes-streets/{z}/{x}/{y}.png" maxzoom="20" 
            visible="true" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" /> 
        <baselayer  type="OSM" id="pitney-bowes-dark" label="Pitney Bows Dark" title="CartoDb" thumbgallery="img/basemap/pitney-bowes-dark.png" 
            url="http://{a-c}.basemaps.cartocdn.com/pitney-bowes-dark/{z}/{x}/{y}.png" maxzoom="20" 
            visible="true" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />    
        <baselayer  type="OSM" id="osm1" label="OpenStreetMap" title="OpenStreetMap" thumbgallery="img/basemap/osm.png" 
            url="http://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png" 
            attribution="© MapQuest. Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'>OpenStreetMap &lt;/a>,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a>" visible="false"/>        
    </baselayers>     
    <proxy url=""/>    
    <olscompletion url="http://api-adresse.data.gouv.fr/search/" type="ban"/>
    <searchparameters bbox="true" localities="true" features="false" /> 

    <themes>            
        <!--Theme is large group of themes or layers directly -->
        <theme name="Service postal"   collapsed="true" id="phase" icon="envelope">             
                <layer id="upap_demo_cameroun:bureaux_postaux" name="Bureaux de postes"  visible="true"  queryable="true" url="http://geoserver.metis-map.fr/geoserver/wms" infoformat="application/vnd.ogc.gml">
                    <template url="/templates/demo.poste.mst" />
                </layer>
                <layer id="upap_demo_cameroun:centres_tri" name="Centres de tri (A venir)"  visible="false" url="http://geoserver.metis-map.fr/geoserver/wms" />
                <layer  id="upap_demo_cameroun:bureaux" name="Bureaux adminsitratifs (A venir)"  visible="false" url="http://geoserver.metis-map.fr/geoserver/wms" />    
        </theme>   

        <theme name="Banques d'adresses collectes"  collapsed="true" id="banque_donnees" icon="map-marker">    
            <layer id="upap_demo_cameroun:donnees_collectees" name="Données collectées"  visible="false"  queryable="true" fields="type_bal" aliases="Nature" infoformat="application/vnd.ogc.gml" url="http://geoserver.metis-map.fr/geoserver/wms">                
            </layer>
            <layer id="upap_demo_cameroun:foyers_collectees" name="Foyers"  visible="false"  queryable="false" url="http://geoserver.metis-map.fr/geoserver/wms"/>  
            <layer  id="Temp" name="Domiciles professionnels"  visible="false"/>  
        </theme>

        <theme name="Commerces" collapsed="true" id="analyse" icon="shopping-bag">
            <layer id="upap_demo_cameroun:commerces" name="Commerces"  visible="false"  queryable="true" fields="type_comm" aliases="Type de commerce" infoformat="application/vnd.ogc.gml" url="http://geoserver.metis-map.fr/geoserver/wms"/>  
            <layer id="upap_demo_cameroun:zones_tampon_commerces" name="Zones tampon commerces"  visible="false"   queryable="false" url="http://geoserver.metis-map.fr/geoserver/wms"/>
            <layer id="upap_demo_cameroun:densite_tampon" name="Densité tampon"  visible="false"   queryable="false" url="http://geoserver.metis-map.fr/geoserver/wms"/>          
        </theme>

        <theme name="Socle" collapsed="false" id="socle" icon="globe">            
            <layer id="upap_demo_cameroun:batiments"  name="Batiments" visible="false" queryable="true" fields="nom" aliases="Nom" infoformat="application/vnd.ogc.gml" url="http://geoserver.metis-map.fr/geoserver/wms"/>
            <layer id="upap_demo_cameroun:routes"  name="Routes" visible="true" url="http://geoserver.metis-map.fr/geoserver/wms" 
            queryable="true" fields="name" aliases="Nom" infoformat="application/vnd.ogc.gml" featurecount="2"/>                                 
        </theme>
        
        <theme name="Analyses thématiques" collapsed="true" id="isochrone" icon="cogs">
            <layer id="upap_demo_cameroun:grille_densite_pop" name="Grille de densité de population"  visible="false"  queryable="true" fields="range" aliases="Intervalles de classes" infoformat="application/vnd.ogc.gml" url="http://geoserver.metis-map.fr/geoserver/wms"/> 
            <layer id="upap_demo_cameroun:grille_densite_relais" name="Grille de densité des points relais"  visible="false"  queryable="true" fields="range" aliases="Intervalles de classes" infoformat="application/vnd.ogc.gml" url="http://geoserver.metis-map.fr/geoserver/wms"/> 		
            <layer id="upap_demo_cameroun:isochrones_bureau_poste" name="Isochrones du bureau de poste"  visible="false"   queryable="false" 
            url="http://geoserver.metis-map.fr/geoserver/wms"/>   
            <layer id="upap_demo_cameroun:isochrones_bureau_poste_djemoun" name="Isochrones du bureau de poste de Djemoun"  visible="false"   queryable="false" 
             url="http://geoserver.metis-map.fr/geoserver/wms"/> 
             <layer id="upap_demo_cameroun:itineraire_footwalking_poste_to_djemoun_agg" name="Itinéraire poste a poste a pied"  visible="false"   queryable="false" 
            url="http://geoserver.metis-map.fr/geoserver/wms"/>  
            <layer id="upap_demo_cameroun:itineraire_cycling_poste_to_djemoun_agg" name="Itinéraire poste a poste en velo"  visible="false"   queryable="false" 
            url="http://geoserver.metis-map.fr/geoserver/wms"/> 
            <layer id="upap_demo_cameroun:itineraire_cardriving_poste_to_djemoun_agg" name="Itinéraire poste a poste en voiture"  visible="false"   queryable="false" 
            url="http://geoserver.metis-map.fr/geoserver/wms"/> 
            <layer id="upap_demo_cameroun:itineraire_footwalking_poste" name=  "Itinéraire poste a point relais a pied"  visible="false"   queryable="false" 
            url="http://geoserver.metis-map.fr/geoserver/wms"/> 
            <layer id="upap_demo_cameroun:itineraire_cyclingroad_poste" name="Itinéraire poste a point relais en velo"  visible="false"   queryable="false" 
            url="http://geoserver.metis-map.fr/geoserver/wms"/> 
            <layer id="upap_demo_cameroun:itineraire_cardriving_poste" name="Itinéraire poste a point relais en voiture"  visible="false"   queryable="false" 
            url="http://geoserver.metis-map.fr/geoserver/wms"/> 
        </theme>
    </themes>
</config>
